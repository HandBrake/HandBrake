msgstr "Bordo inferiore"

#: src/ghb3.ui:4102
msgid "Right Pad"
msgstr "Bordo destro"

#: src/ghb3.ui:4118
msgid "Color:"
msgstr "Colore:"

#: src/ghb3.ui:4135
msgid "Color of border added around video."
msgstr "Colore del bordo aggiunto attorno al video."

#: src/ghb3.ui:4157
msgid "<b>Borders</b>"
msgstr "<b>Bordi</b>"

#: src/ghb3.ui:4255 src/ghb3.ui:4287 src/ghb3.ui:4305
