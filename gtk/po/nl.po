msgstr "Opvulling rechts"

#: src/ghb3.ui:4058
msgid "Color:"
msgstr "Kleur:"

#: src/ghb3.ui:4075
msgid "Color of border added around video."
msgstr "Kleur van de rand om de video."

#: src/ghb3.ui:4097
msgid "<b>Borders</b>"
msgstr "<b>Randen</b>"

#: src/ghb3.ui:4195 src/ghb3.ui:4227 src/ghb3.ui:4245
