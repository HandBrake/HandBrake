msgstr "右埋め"

#: src/ui/ghb.ui:2649
msgid "Color:"
msgstr "色:"

#: src/ui/ghb.ui:2661
msgid "Color of border added around video."
msgstr "映像の周りに追加される境界線の色です。"

#: src/ui/ghb.ui:2685
msgid "<b>Final Dimensions</b>"
msgstr "<b>最終解像度</b>"

#: src/ui/ghb.ui:2737 src/ui/ghb.ui:2760 src/ui/ghb.ui:2773
