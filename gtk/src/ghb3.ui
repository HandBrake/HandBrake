  <object class="GtkFileFilter" id="SourceFilterEVO">
    <patterns>
      <pattern>*.evo</pattern>
      <pattern>*.EVO</pattern>
    </patterns>
  </object>
  <object class="GtkFileFilter" id="SourceFilterFLV">
    <patterns>
      <pattern>*.flv</pattern>
      <pattern>*.FLV</pattern>
    </patterns>
  </object>
  <object class="GtkFileFilter" id="SourceFilterMKV">
    <patterns>
      <pattern>*.mkv</pattern>
      <pattern>*.MKV</pattern>
    </patterns>
  </object>
