<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkFileFilter" id="SourceFilterEVO">
    <patterns>
      <pattern>*.evo</pattern>
      <pattern>*.EVO</pattern>
    </patterns>
  </object>
  <object class="GtkFileFilter" id="SourceFilterFLV">
    <mime-types>
      <mime-type>video/x-flv</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterMKV">
    <mime-types>
      <mime-type>video/x-matroska</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterWebM">
    <mime-types>
      <mime-type>video/webm</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterMOV">
    <mime-types>
      <mime-type>video/quicktime</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterAVI">
    <mime-types>
      <mime-type>video/x-ms-video</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterMP4">
    <mime-types>
      <mime-type>video/mp4</mime-type>
      <mime-type>video/3gpp</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterMPG">
    <mime-types>
      <mime-type>video/mpeg</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterOGG">
    <mime-types>
      <mime-type>video/ogg</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterTS">
    <mime-types>
      <mime-type>video/mp2t</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterVOB">
    <patterns>
      <pattern>*.VOB</pattern>
      <pattern>*.vob</pattern>
    </patterns>
  </object>
  <object class="GtkFileFilter" id="SourceFilterVideo">
    <mime-types>
      <mime-type>video/*</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterWMV">
    <mime-types>
      <mime-type>video/x-ms-wmv</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="SourceFilterAll">
    <mime-types>
      <mime-type>application/octet-stream</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="FilterAll">
    <patterns>
      <pattern>*</pattern>
    </patterns>
  </object>
  <object class="GtkFileFilter" id="FilterJSON">
    <mime-types>
      <mime-type>application/json</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="FilterXML">
    <mime-types>
      <mime-type>application/xml</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileFilter" id="FilterPlist">
    <patterns>
      <pattern>*.plist</pattern>
      <pattern>*.PLIST</pattern>
    </patterns>
  </object>
  <object class="GtkLabel" id="SubBurnedLabel">
    <property name="tooltip-text" translatable="yes">Render the subtitle over the video.

The subtitle will be part of the video and can not be disabled.</property>
    <property name="label" translatable="yes">&lt;b&gt;Burned In&lt;/b&gt;</property>
    <property name="use-markup">1</property>
  </object>
  <object class="GtkLabel" id="SubDefaultLabel">
    <property name="tooltip-text" translatable="yes">Set the default output subtitle track.

Most players will automatically display this
subtitle track whenever the video is played.

This is useful for creating a &quot;forced&quot; track
in your output.</property>
    <property name="label" translatable="yes">&lt;b&gt;Default&lt;/b&gt;</property>
    <property name="use-markup">1</property>
  </object>
  <object class="GtkLabel" id="SubForcedLabel">
    <property name="tooltip-text" translatable="yes">Use only subtitles that have been flagged
as forced in the source subtitle track

&quot;Forced&quot; subtitles are usually used to show
subtitles during scenes where someone is speaking
a foreign language.</property>
    <property name="label" translatable="yes">&lt;b&gt;Forced Only&lt;/b&gt;</property>
    <property name="use-markup">1</property>
  </object>
  <object class="GtkLabel" id="SubSRTOffsetLabel">
    <property name="tooltip-text" translatable="yes">Add (or subtract) an offset (in milliseconds)
to the start of the SRT subtitle track.

Often, the start of an external SRT file
does not coincide with the start of the video.
This setting allows you to synchronize the files.</property>
    <property name="label" translatable="yes">&lt;b&gt;SRT Offset&lt;/b&gt;</property>
    <property name="use-markup">1</property>
  </object>
  <object class="GtkLabel" id="SubTrackLabel">
    <property name="tooltip-text" translatable="yes">The source subtitle track

You can choose any of the subtitles
recognized in your source file.

In addition, there is a special track option
&quot;Foreign Audio Scan&quot;.  This option will add
an extra pass to the encode that searches for
subtitles that may correspond to a foreign
language scene.  This option is best used in
conjunction with the &quot;Forced&quot; option.</property>
    <property name="label" translatable="yes">&lt;b&gt;Track&lt;/b&gt;</property>
    <property name="use-markup">1</property>
  </object>
  <object class="GtkWindow" id="presets_window">
    <property name="title" translatable="yes">Presets</property>
    <property name="icon-name">fr.handbrake.ghb</property>
    <property name="default-width">300</property>
    <property name="default-height">600</property>
    <property name="hide-on-close">True</property>
    <signal name="notify::default-height" handler="presets_window_save_size_cb" swapped="no"/>
    <signal name="notify::default-width" handler="presets_window_save_size_cb" swapped="no"/>
    <child type="titlebar">
      <object class="GtkHeaderBar" id="presets-header-bar">
        <child>
          <object class="GtkButton" id="preset-save-as">
            <property name="tooltip-text" translatable="yes">Save as new preset</property>
            <property name="icon-name">bookmark-new-symbolic</property>
            <property name="action-name">app.preset-save-as</property>
          </object>
        </child>
        <child type="end">
          <object class="GtkMenuButton" id="preset-actions-menu">
            <property name="primary">True</property>
            <property name="icon-name">view-more-symbolic</property>
            <property name="tooltip-text" translatable="yes">Actions</property>
            <property name="menu-model">preset-actions-menu-model</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkScrolledWindow" id="presets_scroll">
        <property name="min-content-width">200</property>
        <property name="vexpand">1</property>
        <property name="child">
          <object class="GtkTreeView" id="presets_list">
            <property name="width-request">206</property>
            <property name="focusable">1</property>
            <property name="headers-visible">0</property>
            <child internal-child="selection">
              <object class="GtkTreeSelection" id="treeview-selection4"/>
            </child>
            <child>
              <object class="GtkPopoverMenu" id="preset-context-menu">
                <property name="has-arrow">False</property>
                <property name="halign">start</property>
                <property name="valign">end</property>
                <property name="visible">false</property>
                <property name="menu-model">preset-context-menu-model</property>
              </object>
            </child>
            <child>
              <object class="GtkGestureClick">
                <property name="button">3</property>
                <signal name="pressed" handler="presets_list_context_menu_cb" swapped="no"/>
              </object>
            </child>
          </object>
        </property>
      </object>
    </child>
  </object>
  <object class="GtkWindow" id="queue_window">
    <property name="title" translatable="yes">HandBrake Queue</property>
    <property name="icon-name">fr.handbrake.ghb</property>
    <signal name="close-request" handler="queue_window_delete_cb" swapped="no"/>
    <child type="titlebar">
      <object class="GtkHeaderBar">
        <child>
          <object class="GhbButton" id="queue_list_start">
            <property name="tooltip-text" translatable="yes">Start Encoding</property>
            <property name="label" translatable="yes">Start</property>
            <property name="icon-name">hb-start-symbolic</property>
            <property name="action-name">app.queue-start</property>
            <property name="orientation">horizontal</property>
          </object>
        </child>
        <child>
          <object class="GhbButton" id="queue_list_pause">
            <property name="tooltip-text" translatable="yes">Pause Encoding</property>
            <property name="label" translatable="yes">Pause</property>
            <property name="icon-name">hb-pause-symbolic</property>
            <property name="action-name">app.queue-pause</property>
            <property name="orientation">horizontal</property>
          </object>
        </child>
        <child type="title">
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <property name="valign">center</property>
            <property name="css-classes">ghb-window-title</property>
            <child>
              <object class="GtkLabel">
                <property name="label" translatable="yes">Queue</property>
                <property name="css-classes">title</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="queue_status_label">
                <property name="label" translatable="yes">0 jobs pending</property>
                <property name="css-classes">subtitle</property>
              </object>
            </child>
          </object>
        </child>
        <child type="end">
          <object class="GtkMenuButton" id="queue_options_menu_button">
            <property name="menu-model">queue-options-menu-model</property>
            <property name="icon-name">view-more-symbolic</property>
            <property name="tooltip-text" translatable="yes">Options</property>
            <property name="css-classes">flat</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox" id="queue_tab">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkPaned" id="queue_paned">
            <property name="shrink-end-child">0</property>
            <property name="shrink-start-child">0</property>
            <property name="position">500</property>
            <property name="start-child">
              <object class="GtkBox">
                <property name="orientation">vertical</property>
                <property name="hexpand">1</property>
                <property name="css-classes">ghb-queue-panel</property>
                <child>
                  <object class="GtkBox">
                    <property name="orientation">vertical</property>
                    <property name="hexpand">1</property>
                    <child>
                      <object class="GtkGestureClick">
                        <property name="button">0</property>
                        <signal name="pressed" handler="queue_button_press_cb" object="queue_list_window" swapped="no"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkPopoverMenu" id="queue_context_menu">
                        <property name="has-arrow">False</property>
                        <property name="halign">start</property>
                        <property name="valign">end</property>
                        <property name="visible">false</property>
                        <property name="menu-model">queue-context-menu-model</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkScrolledWindow" id="queue_list_window">
                        <property name="hexpand">1</property>
                        <property name="hscrollbar-policy">never</property>
                        <property name="min-content-height">440</property>
                        <child>
                          <object class="GtkListBox" id="queue_list">
                            <property name="focusable">1</property>
                            <property name="vexpand">1</property>
                            <property name="hexpand">1</property>
                            <property name="selection-mode">multiple</property>
                            <property name="activate-on-single-click">0</property>
                            <signal name="row-selected" handler="queue_list_selection_changed_cb" swapped="no"/>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkActionBar">
                    <property name="halign">fill</property>
                    <child type="center">
                      <object class="GtkBox">
                        <property name="orientation">horizontal</property>
                        <property name="spacing">4</property>
                        <property name="hexpand">False</property>
                        <property name="halign">center</property>
                        <child>
                          <object class="GtkLabel" id="queue_done_label">
                            <property name="hexpand">1</property>
                            <property name="halign">start</property>
                            <property name="use-markup">1</property>
                            <property name="label" translatable="yes">When Done:</property>
                          </object>
                        </child>
                        <child type="center">
                          <object class="GtkComboBox" id="QueueWhenComplete">
                            <signal name="changed" handler="temp_when_complete_changed_cb" swapped="no"/>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child type="end">
                      <object class="GtkToggleButton">
                        <property name="icon-name">sidebar-show-right-symbolic</property>
                        <property name="tooltip-text" translatable="yes">Show/hide the queue details panel</property>
                        <property name="action-name">app.queue-show-sidebar</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </property>
            <property name="end-child">
              <object class="GtkBox" id="queue_sidebar">
                <property name="visible">False</property>
                <property name="orientation">vertical</property>
                <property name="margin-top">12</property>
                <property name="css-classes">ghb-queue-panel</property>
                <child>
                  <object class="GtkStackSwitcher" id="QueueStackSwitcher">
                    <property name="stack">QueueStack</property>
                    <property name="hexpand">1</property>
                    <property name="valign">start</property>
                    <property name="margin-start">12</property>
                    <property name="margin-end">12</property>
                  </object>
                </child>
                <child>
                  <object class="GtkStack" id="QueueStack">
                    <property name="vexpand">1</property>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">queue_summary_tab</property>
                        <property name="title" translatable="yes">Summary</property>
                        <property name="child">
                          <object class="GtkBox" id="queue_summary_tab">
                            <property name="orientation">vertical</property>
                            <property name="hexpand">1</property>
                            <property name="vexpand">1</property>
                            <property name="spacing">3</property>
                            <child>
                              <object class="GtkBox" id="queue_summary_toolbar">
                                <property name="css-classes">toolbar</property>
                                <property name="hexpand">1</property>
                                <property name="halign">fill</property>
                                <property name="margin-top">6</property>
                                <property name="margin-start">6</property>
                                <property name="margin-end">6</property>
                                <property name="can-focus">0</property>
                                <child>
                                  <object class="GtkButton" id="queue_reload">
                                    <property name="tooltip-text" translatable="yes">Mark selected queue entry as pending.
Resets the queue job to pending and ready to run again.</property>
                                    <property name="action-name">app.queue-reset</property>
                                    <property name="css-classes">flat</property>
                                    <property name="child">
                                      <object class="GtkBox">
                                        <property name="spacing">6</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">view-refresh-symbolic</property>
                                            <property name="accessible-role">presentation</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Reset</property>
                                          </object>
                                        </child>
                                      </object>
                                    </property>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="queue_edit">
                                    <property name="action-name">app.queue-edit</property>
                                    <property name="css-classes">flat</property>
                                    <property name="child">
                                      <object class="GtkBox">
                                        <property name="spacing">6</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">document-edit-symbolic</property>
                                            <property name="accessible-role">presentation</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Edit</property>
                                            <property name="accessible-role">presentation</property>
                                          </object>
                                        </child>
                                      </object>
                                    </property>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkMenuButton" id="queue_actions_menu_button">
                                    <property name="menu-model">queue-actions-menu-model</property>
                                    <property name="label" translatable="yes">Actions</property>
                                    <property name="hexpand">0</property>
                                    <property name="css-classes">flat</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkScrolledWindow" id="queue_summary_scroll">
                                <property name="focusable">1</property>
                                <property name="vexpand">1</property>
                                <property name="hscrollbar-policy">never</property>
                                <property name="child">
                                  <object class="GtkGrid" id="queue_summary_grid">
                                    <property name="vexpand">1</property>
                                    <property name="hexpand">0</property>
                                    <property name="column-spacing">12</property>
                                    <property name="row-spacing">12</property>
                                    <property name="margin-start">12</property>
                                    <property name="margin-end">12</property>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_preset_label">
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="halign">start</property>
                                        <property name="use-markup">1</property>
                                        <property name="label" translatable="yes">Preset:</property>
                                        <layout>
                                          <property name="row">0</property>
                                          <property name="column">0</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_preset">
                                        <property name="halign">start</property>
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="hexpand">0</property>
                                        <property name="label" translatable="yes"></property>
                                        <property name="width-chars">30</property>
                                        <layout>
                                          <property name="row">0</property>
                                          <property name="column">1</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_source_label">
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="halign">start</property>
                                        <property name="use-markup">1</property>
                                        <property name="label" translatable="yes">Source:</property>
                                        <layout>
                                          <property name="row">1</property>
                                          <property name="column">0</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_source">
                                        <property name="halign">start</property>
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="hexpand">0</property>
                                        <property name="label" translatable="yes"></property>
                                        <property name="width-chars">30</property>
                                        <property name="lines">2</property>
                                        <property name="wrap-mode">word-char</property>
                                        <property name="ellipsize">middle</property>
                                        <property name="selectable">1</property>
                                        <property name="margin-bottom">12</property>
                                        <layout>
                                          <property name="row">1</property>
                                          <property name="column">1</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_title_label">
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="halign">start</property>
                                        <property name="use-markup">1</property>
                                        <property name="label" translatable="yes">Title:</property>
                                        <layout>
                                          <property name="row">2</property>
                                          <property name="column">0</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_title">
                                        <property name="halign">start</property>
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="hexpand">0</property>
                                        <property name="label" translatable="yes"></property>
                                        <property name="width-chars">30</property>
                                        <property name="lines">2</property>
                                        <property name="wrap-mode">word-char</property>
                                        <property name="ellipsize">middle</property>
                                        <property name="selectable">1</property>
                                        <layout>
                                          <property name="row">2</property>
                                          <property name="column">1</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_dest_label">
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="halign">start</property>
                                        <property name="use-markup">1</property>
                                        <property name="label" translatable="yes">Destination:</property>
                                        <layout>
                                          <property name="row">3</property>
                                          <property name="column">0</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_dest">
                                        <property name="halign">start</property>
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="hexpand">0</property>
                                        <property name="label" translatable="yes"></property>
                                        <property name="width-chars">30</property>
                                        <property name="lines">2</property>
                                        <property name="wrap-mode">word-char</property>
                                        <property name="ellipsize">middle</property>
                                        <property name="selectable">1</property>
                                        <property name="margin-bottom">12</property>
                                        <layout>
                                          <property name="row">3</property>
                                          <property name="column">1</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_dimensions_label">
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="halign">start</property>
                                        <property name="use-markup">1</property>
                                        <property name="label" translatable="yes">Dimensions:</property>
                                        <layout>
                                          <property name="row">4</property>
                                          <property name="column">0</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_dimensions">
                                        <property name="halign">start</property>
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="hexpand">0</property>
                                        <property name="label" translatable="yes"></property>
                                        <property name="width-chars">30</property>
                                        <layout>
                                          <property name="row">4</property>
                                          <property name="column">1</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_video_label">
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="halign">start</property>
                                        <property name="use-markup">1</property>
                                        <property name="label" translatable="yes">Video:</property>
                                        <layout>
                                          <property name="row">5</property>
                                          <property name="column">0</property>
                                        </layout>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel" id="queue_summary_video">
                                        <property name="halign">start</property>
                                        <property name="xalign">0</property>
                                        <property name="yalign">0</property>
                                        <property name="hexpand">0</property>
                                        <property name="label" translatable="yes"></property>
                                        <property name="width-chars">30</property>
                                        <layout>
                                          <property name="row">5</property>
                                          <property name="column">1</property>
                                        </layout>
                                      </object>
                                    </child>
