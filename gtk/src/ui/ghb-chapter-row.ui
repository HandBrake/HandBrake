            <property name="xalign">1.0</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="duration_label">
            <property name="width-chars">10</property>
            <property name="xalign">1.0</property>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="chapter_entry">
