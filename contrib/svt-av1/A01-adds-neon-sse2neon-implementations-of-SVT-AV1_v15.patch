+// 8 bit
+////////////////////////////////////////////////////////////////////////////////
+// 2 tap bilinear filters
+#define BIL_SUBPEL_BITS 3
+#define BIL_SUBPEL_SHIFTS (1 << BIL_SUBPEL_BITS)
+static const uint8_t bilinear_filters_2t[BIL_SUBPEL_SHIFTS][2] = {
+    {128, 0},
+    {112, 16},
+    {96, 32},
+    {80, 48},
+    {64, 64},
+    {48, 80},
+    {32, 96},
+    {16, 112},
